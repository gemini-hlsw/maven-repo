    <modelVersion>4.0.0</modelVersion>
    <groupId>edu.gemini.aspen</groupId>
    <artifactId>giapi-osgi</artifactId>
    <version>14.4.6</version>

    <name>giapi-osgi</name>

    <description>GIAPI - OSGi</description>

    <properties>
        <!-- OSGi service properties -->
        <org.apache.felix.http.jettyEnabled>true</org.apache.felix.http.jettyEnabled>
        <org.osgi.service.http.port.secure>8443</org.osgi.service.http.port.secure>
        <org.osgi.service.http.port>8888</org.osgi.service.http.port>
        <osgi.shell.telnet.ip>0.0.0.0</osgi.shell.telnet.ip>
        <osgi.shell.telnet.port>15001</osgi.shell.telnet.port>
        <org.osgi.framework.system.packages.extra>javax.xml.parsers,com.sun.jini,com.sun.javadoc,sun.io,sun.swing,sun.misc,com.sun,sun.security.action,edu.gemini.rmi.server,sun.rmi.runtime,com.sun.xml.internal.bind</org.osgi.framework.system.packages.extra>
        <org.osgi.framework.bootdelegation>com.sun.xml.internal.bind</org.osgi.framework.bootdelegation>
        <vmo.settings>-Xverify:none -XX:+UnlockDiagnosticVMOptions -XX:+UnsyncloadClass</vmo.settings>

        <!-- Logging properties -->
        <logs.dir>logs</logs.dir>
        <felix.cm.loglevel>1</felix.cm.loglevel>
        <felix.log.level>0</felix.log.level>
        <felix.fileinstall.log.level>1</felix.fileinstall.log.level>
        <felix.fileinstall.noInitialDelay>true</felix.fileinstall.noInitialDelay>
        <ipojo.log.level>warn</ipojo.log.level>
        <org.ops4j.pax.logging.DefaultServiceLog.level>WARN</org.ops4j.pax.logging.DefaultServiceLog.level>

        <!-- Configuration dir -->
        <conf.base>../src/main/config</conf.base>
        <felix.fileinstall.dir>${conf.base}/services</felix.fileinstall.dir>

        <!-- Component versions -->
        <clover.version>3.1.0</clover.version>
        <org.activemq.core.version>5.5.1</org.activemq.core.version>
        <pax-web.version>1.1.2</pax-web.version>
        <pax-logging.version>1.7.2</pax-logging.version>
        <pax-runner.version>1.8.6</pax-runner.version>
        <org.apache.felix.ipojo>1.8.0</org.apache.felix.ipojo>
        <scala-lang.version>2.10.4</scala-lang.version>
        <scalatest.version>2.10</scalatest.version>
        <scala-plugin.version>3.1.6</scala-plugin.version>
        <vaadin.version>6.8.2</vaadin.version>
        <junit.version>4.10</junit.version>
        <felix.framework.version>4.2.1</felix.framework.version>

        <gmp.version>${project.version}</gmp.version>
        <!-- Global encoding -->
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
    </properties>

    <packaging>pom</packaging>

    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <version>2.3.2</version>
                <executions>
                    <execution>
                        <phase>compile</phase>
                        <goals>
                            <goal>compile</goal>
                        </goals>
                    </execution>
                </executions>
                <configuration>
                    <source>1.6</source>
                    <target>1.6</target>
                </configuration>
            </plugin>
            <plugin>
                <groupId>com.atlassian.maven.plugins</groupId>
                <artifactId>maven-clover2-plugin</artifactId>
                <version>${clover.version}</version>
                <configuration>
                    <licenseLocation>${user.home}/.clover/clover.license</licenseLocation>
                    <!--<targetPercentage>30%</targetPercentage>
                    <methodContexts>
                        <main>(.* )?public static void main\(String\[\] argv\).*</main>
                    </methodContexts>
                    <contextFilters>main</contextFilters>-->
                    <historyThreshold>0.5%</historyThreshold>
                    <generateHistorical>true</generateHistorical>
                    <historyDir>${project.build.directory}/../history</historyDir>
                </configuration>
            </plugin>

            <!-- Can support launching in felix -->
            <plugin>
                <groupId>org.ops4j</groupId>
                <artifactId>maven-pax-plugin</artifactId>
                <version>1.4</version>
                <configuration>
                    <provision>
                        <param>--keepOriginalUrls</param>
                        <param>--debug=warn</param>
                        <param>--vmo=${vmo.settings}</param>
                        <!--param>-vmo=-Xverify:none -XX:+UnlockDiagnosticVMOptions -XX:+UnsyncloadClass -XX:+UnlockExperimentalVMOptions -XX:+UseG1GC</param-->
                        <param>--bd=com.sun.xml.internal.bind</param>
                        <!--param>-version=${felix.framework.version}</param-->
                    </provision>
                    <framework>felix</framework>
                    <runner>${pax-runner.version}</runner>
                    <snapshots>true</snapshots>
                </configuration>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-failsafe-plugin</artifactId>
                <version>2.7.1</version>
                <configuration>
                    <forkMode>always</forkMode>
                </configuration>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-surefire-plugin</artifactId>
                <version>2.10</version>
                <configuration>
                    <argLine>-Xverify:none</argLine>
                </configuration>
            </plugin>
            <!-- Create an assembly -->
            <plugin>
                <artifactId>maven-assembly-plugin</artifactId>
                <version>2.3</version>
            </plugin>
            <!-- Define properties for the release plugin -->
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-release-plugin</artifactId>
                <version>2.3.1</version>
                <configuration>
                    <tagNameFormat>GMP-R@{project.version}</tagNameFormat>
                    <remoteTagging>false</remoteTagging>
                </configuration>
            </plugin>

        </plugins>
    </build>

