    <modelVersion>4.0.0</modelVersion>
    <groupId>edu.gemini.aspen</groupId>
    <artifactId>giapi-osgi</artifactId>
    <version>14.3.2</version>

    <name>giapi-osgi</name>

    <description>GIAPI - OSGi</description>

    <properties>
        <!-- OSGi service properties -->
        <org.apache.felix.http.jettyEnabled>true</org.apache.felix.http.jettyEnabled>
        <org.osgi.service.http.port.secure>8443</org.osgi.service.http.port.secure>
        <org.osgi.service.http.port>8888</org.osgi.service.http.port>
        <osgi.shell.telnet.ip>0.0.0.0</osgi.shell.telnet.ip>
        <jini.lus.import.hosts>gsodbtest.cl.gemini.edu</jini.lus.import.hosts>
        <jini.lus.import.groups>test</jini.lus.import.groups>
        <osgi.shell.telnet.port>15001</osgi.shell.telnet.port>
        <org.osgi.framework.system.packages.extra>javax.xml.parsers,com.sun.jini,com.sun.javadoc,sun.io,sun.swing,sun.misc,com.sun,sun.security.action,edu.gemini.rmi.server,sun.rmi.runtime</org.osgi.framework.system.packages.extra>
        <vmo.settings>-Xverify:none -XX:+UnlockDiagnosticVMOptions -XX:+UnsyncloadClass</vmo.settings>

        <!-- Logging properties -->
        <logs.dir>logs</logs.dir>
        <felix.cm.loglevel>1</felix.cm.loglevel>
        <felix.log.level>0</felix.log.level>
        <felix.fileinstall.log.level>1</felix.fileinstall.log.level>
        <ipojo.log.level>warn</ipojo.log.level>
        <org.ops4j.pax.logging.DefaultServiceLog.level>WARN</org.ops4j.pax.logging.DefaultServiceLog.level>

        <!-- Configuration dir -->
        <conf.base>../src/main/config</conf.base>
        <felix.fileinstall.dir>${conf.base}/services</felix.fileinstall.dir>

        <!-- Component versions -->
        <clover.version>3.1.0</clover.version>
        <org.activemq.core.version>5.5.1</org.activemq.core.version>
        <pax-web.version>1.1.2</pax-web.version>
        <pax-logging.version>1.6.5</pax-logging.version>
        <pax-runner.version>1.7.6</pax-runner.version>
        <org.apache.felix.ipojo>1.8.0</org.apache.felix.ipojo>
        <scala-lang.version>2.9.1</scala-lang.version>
        <scala-plugin.version>3.1.0</scala-plugin.version>
        <vaadin.version>6.8.2</vaadin.version>
        <junit.version>4.10</junit.version>
        <felix.framework.version>4.0.2</felix.framework.version>

        <gmp.version>${project.version}</gmp.version>
        <!-- Global encoding -->
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
    </properties>

    <packaging>pom</packaging>

