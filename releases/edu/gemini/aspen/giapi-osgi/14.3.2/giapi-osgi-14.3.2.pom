            <!-- Can support launching in felix -->
            <plugin>
                <groupId>org.ops4j</groupId>
                <artifactId>maven-pax-plugin</artifactId>
                <version>1.4</version>
                <configuration>
                    <provision>
                        <param>--keepOriginalUrls</param>
                        <param>--debug=warn</param>
                        <param>--vmo=${vmo.settings}</param>
                        <!--param>-vmo=-Xverify:none -XX:+UnlockDiagnosticVMOptions -XX:+UnsyncloadClass -XX:+UnlockExperimentalVMOptions -XX:+UseG1GC</param-->
                        <param>--bd=java.rmi.server</param>
                        <!--param>-version=${felix.framework.version}</param-->
                    </provision>
                    <framework>felix</framework>
                    <runner>${pax-runner.version}</runner>
                    <snapshots>true</snapshots>
                </configuration>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-failsafe-plugin</artifactId>
                <version>2.7.1</version>
                <configuration>
                    <forkMode>always</forkMode>
                </configuration>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-surefire-plugin</artifactId>
                <version>2.10</version>
                <configuration>
                    <argLine>-Xverify:none</argLine>
                </configuration>
            </plugin>
            <!-- Create an assembly -->
            <plugin>
                <artifactId>maven-assembly-plugin</artifactId>
                <version>2.3</version>
            </plugin>
            <!-- Define properties for the release plugin -->
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-release-plugin</artifactId>
                <version>2.3.1</version>
                <configuration>
                    <tagNameFormat>GMP-R@{project.version}</tagNameFormat>
                    <remoteTagging>false</remoteTagging>
                </configuration>
            </plugin>

        </plugins>
