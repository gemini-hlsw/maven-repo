    <properties>
        <bundle.symbolicName>edu.gemini.aspen.gmp.commands.model</bundle.symbolicName>
        <bundle.namespace>edu.gemini.aspen.gmp.commands.model</bundle.namespace>
    </properties>

    <modelVersion>4.0.0</modelVersion>
    <groupId>edu.gemini.aspen.gmp</groupId>
    <artifactId>gmp-commands</artifactId>
    <version>1.1.9</version>

    <name>GMP Sequence Commands Service</name>
    <description>Sequence Command Processor service</description>

    <packaging>bundle</packaging>

    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.felix</groupId>
                <artifactId>maven-ipojo-plugin</artifactId>
